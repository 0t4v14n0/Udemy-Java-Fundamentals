public class Converss√£o_C_para_F {

	public static void main(String[] args) {
		
		final double a = 9/5.0;
		final double f = 32;
		double re = 0.0;
		double c = Double.parseDouble(JOptionPane.showInputDialog(null, "Primeiro numero :"));
		
